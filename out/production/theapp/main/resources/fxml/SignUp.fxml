                           <VBox.margin>
                              <Insets bottom="10.0" left="25.0" right="25.0" top="10.0" />
                           </VBox.margin>
                        </TextField>
                        <TextField fx:id="phonetf" alignment="CENTER" prefHeight="26.0" prefWidth="191.0" promptText="Phone Number">
