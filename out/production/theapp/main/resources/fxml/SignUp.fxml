<?import javafx.scene.text.Font?>

<GridPane alignment="center" maxHeight="400" maxWidth="600" minHeight="400" minWidth="600" style="-fx-background-color: #2b2b2d;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.SignUpController">
