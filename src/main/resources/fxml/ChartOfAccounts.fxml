<AnchorPane prefHeight="600.0" prefWidth="850.0" style="-fx-background-color: #434447;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.ChartOfAccountsController">
   <children>
