                                  <TableColumn fx:id="statusAssetColumn" prefWidth="161.0" text="Archived/Status" />
                                </columns>
                                 <VBox.margin>
                                    <Insets left="5.0" right="5.0" />
                                 </VBox.margin>
                              </TableView>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <Button layoutX="693.0" layoutY="14.0" mnemonicParsing="false" onAction="#addNewAccount" text="Add a New Account" />
   </children>
</AnchorPane>
