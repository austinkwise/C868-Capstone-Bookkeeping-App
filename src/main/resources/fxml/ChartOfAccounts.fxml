        <tabs>
          <Tab text="Assets">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox prefHeight="532.0" prefWidth="806.0">
                           <children>
                              <HBox alignment="CENTER_RIGHT" prefHeight="45.0" prefWidth="806.0">
                                 <children>
                                    <HBox prefHeight="45.0" prefWidth="711.0">
                                       <children>
                                          <Label prefHeight="45.0" prefWidth="217.0" text="Assets" textFill="WHITE" />
                                       </children>
                                    </HBox>
                                    <Button mnemonicParsing="false" onAction="#editAssetAccount" text="Edit">
